{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/api-pack": "^1.1.0",
        "symfony/console": "*",
        "symfony/flex": "^1.1.0",
        "symfony/framework-bundle": "*",
        "symfony/orm-pack": "^1.0.5",
        "symfony/yaml": "*",
        "webonyx/graphql-php": "^0.12.5"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.3",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/dotenv": "*",
        "symfony/maker-bundle": "^1.6",
        "symfony/phpunit-bridge": "*",
        "symfony/profiler-pack": "^1.0.3",
        "symfony/test-pack": "^1.0.2",
        "symfony/web-server-bundle": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.1.*"
        }
    }
}
